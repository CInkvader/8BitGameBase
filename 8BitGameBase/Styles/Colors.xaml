<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <SolidColorBrush x:Key="GameBackground">transparent</SolidColorBrush>
    <SolidColorBrush x:Key="DefaultForegroundLight">white</SolidColorBrush>
    <SolidColorBrush x:Key="DefaultColorLight">white</SolidColorBrush>
    <SolidColorBrush x:Key="DefaultColorMedium">#FF5C2E78</SolidColorBrush>
    <SolidColorBrush x:Key="DefaultColorMedium_2">#FF217143</SolidColorBrush>
    <SolidColorBrush x:Key="DefaultColorMedium_3">#FFFFA800</SolidColorBrush>
    <SolidColorBrush x:Key="DefaultForegroundBright">#FFFFA800</SolidColorBrush>
    
    <SolidColorBrush x:Key="BackgroundScreenActive">#FF121212</SolidColorBrush>
    <SolidColorBrush x:Key="TitleBarMinimizedColor">#FF5C2E78</SolidColorBrush>
    <SolidColorBrush x:Key="TitleBarMaximizedColor">black</SolidColorBrush>
</ResourceDictionary>